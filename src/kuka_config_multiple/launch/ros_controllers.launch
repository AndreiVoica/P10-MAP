<?xml version="1.0"?>
<launch>

  <!-- Load joint controller configurations from YAML file to parameter server -->
  <rosparam file="$(find kuka_config_multiple)/config/ros_controllers.yaml" command="load"/>

  <!-- Load the controllers -->
  <node name="controller_spawner" pkg="controller_manager" type="spawner" respawn="false"
    output="screen" args="KUKA1_arm_controller KUKA1_hand_controller KUKA2_arm_controller KUKA2_hand_controller KUKA3_arm_controller KUKA4_arm_controller KUKA5_arm_controller KUKA5_hand_controller "/>

</launch>
