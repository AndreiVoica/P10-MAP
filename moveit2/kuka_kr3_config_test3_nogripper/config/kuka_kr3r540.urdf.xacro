<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="kuka_kr3r540">
    <xacro:arg name="initial_positions_file" default="initial_positions.yaml" />

    <!-- Import kuka_kr3r540 urdf file -->
    <xacro:include filename="$(find kuka_kr3_support)/urdf/kr3r540nogripper.urdf" />

    <!-- Import control_xacro -->
    <xacro:include filename="kuka_kr3r540.ros2_control.xacro" />


    <xacro:kuka_kr3r540_ros2_control name="FakeSystem" initial_positions_file="$(arg initial_positions_file)"/>

</robot>
